{
  "name": "ai-resume-builder",
  "version": "1.0.0",
  "description": "AI-powered resume builder using MERN stack",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
